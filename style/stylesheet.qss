    min-height: 40px;
    margin: 0;
    padding: 0;
}

#rightBarBox #sideTitle {
    padding: 15px 0 10px;
    font-weight:bold;
    qproperty-alignment: AlignCenter;
    border: 0px solid #455A64;
    border-bottom: 1px solid #455A64;
}


