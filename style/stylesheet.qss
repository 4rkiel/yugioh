}

SoloChoice #introText,
JoinChoice #introText,
NetChoice #matchmakingText,
HostChoice #stateBox {
    font-weight: bold;
}

/******************************************************************************

    Options Menu

******************************************************************************/
    
BuildTab,
OptionTab, 
HelpTab, 
RuleTab {
    margin: 0px;
    padding: 0px;
}

BuildTab #tabInside,
OptionTab #tabInside, 
HelpTab #tabInside, 
RuleTab #tabInside {
    background: transparent;
}


BuildTab #buildTab, 
OptionTab #optionTab, 
HelpTab #helpTab, 
RuleTab #ruleTab {
    background: #B0BEC5;
}

BuildTab #buildTab .QPushButton,
OptionTab #optionTab .QPushButton,
HelpTab #helpTab .QPushButton,
RuleTab #ruleTab .QPushButton {
    border: none;
    outline: none;
    background: none;
    color: #37474F;
    padding: 15px 20px 13px;
    margin: 5px 0px 0px;
    border-bottom: 2px solid #3F51B5;
    border-right: 0px;
    border-left: 0px;
}

BuildTab #buildTab .QPushButton:focus,
OptionTab #optionTab .QPushButton:focus,
HelpTab #helpTab .QPushButton:focus,
RuleTab #ruleTab .QPushButton:focus {
    padding: 15px 20px 10px;
    border-bottom: 5px solid #3F51B5;
}

BuildTab #buildTab .QPushButton[down="true"],
OptionTab #optionTab .QPushButton[down="true"],
HelpTab #helpTab .QPushButton[down="true"],
RuleTab #ruleTab .QPushButton[down="true"] {
    color: #263238;
    padding: 15px 20px 10px;
    border-bottom: 5px solid #FFC107;
}

OptionTab #optionTab ShadowButt,
HelpTab #helpTab ShadowButt,
RuleTab #ruleTab ShadowButt {
    padding: 5px;
    margin: 10px;
}

BuildTab #buildBox,
RuleTab #ruleBox, 
OptionTab #optBox,
HelpTab #helpBox {
    background: #E0F2F1;
}

BuildTab #buildBox QScrollArea,
OptionTab #optBox QScrollArea,
HelpTab #helpBox QScrollArea,
RuleTab #ruleBox QScrollArea {
    background: #E0F2F1;
}

OptionTab #optBox #optScrollBox,
OptionTab #optBox #accScrollBox,
RuleTab #ruleBox #ruleScrollBox {
    background: #E0F2F1;
}

OptionTab #optBox QLabel,
HelpTab #helpBox QLabel,
RuleTab #ruleBox QLabel {
    margin: 0 0 30px 0;
    padding: 0 0 30px 0;
    border: 0px solid #B0BEC5;
    border-bottom: 1px solid #B0BEC5;
}

OptionTab #optBox QComboBox,
HelpTab #helpBox QComboBox,
RuleTab #ruleBox QComboBox {
}


BuildTab #shadowPop {
    border: none;
    outline: none;
    border-radius: 0px;
    background: rgba(0,0,0,100);
}


BuildTab #deckEdit {
    background: #E0F2F1;
}



BuildTab #closePop,
BuildTab #addDeck {
    min-width: 50px;
    max-width: 50px;
    min-height: 50px;
    max-height: 50px;
    border-radius: 25px;
    margin: 10px;
}

BuildTab #closePop {
    background: #FFE082;
}

BuildTab #closePop:focus,
BuildTab #closePop:hover {
    background: #FFECB3;
}

BuildTab #closePop:pressed {
    background: #FFD54F;
}


BuildTab #addDeck {
    background: #FF8F00;
}

BuildTab #addDeck:hover,
BuildTab #addDeck:focus {
    background: #FFA000;
}

BuildTab #addDeck:pressed {
    background: #FF6F00;
}


BuildTab #buildBox #DeckSelector {
    background: #E0F2F1;
}

BuildTab #buildBox #DeckSelector #selectRad {
    padding: 0px 20px;
    margin: 0px 10px;
}

BuildTab #buildBox #DeckSelector #selectButt {
    background: #E0F2F1;
    outline:none;
    border-radius:0px;
    border: 0px solid #DDD;
