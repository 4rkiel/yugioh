}

BuildTab #buildBox #DeckSelector #selectButt:hover,
BuildTab #buildBox #DeckSelector #selectButt:focus {
    background: #FFE082;
}

BuildTab #buildBox #DeckSelector #selectButt:pressed {
    background: #FFD54F;

}

BuildTab #buildBox #DeckSelector #selectSep {
    background: #CCC;
}


/*****************************************************************

    App Window

******************************************************************/

Field {
    background: #263238;
}

Field DarkButt,
Field ShadowButt {
    padding: 5px 10px;
    margin: 10px;
    font-size: 20px;
}


Field #infoBox {
    margin: 0px;
}

Field #infoBox > * {
    margin: 0px;
}

Field #infoBox #lifeBox {
    background: #9FA8DA;
}

Field #infoBox #phaseBox {
    background: #C5CAE9;
}

Field #infoBox #lifeBox #lifeLabel {
    font-weight: bold;
}

Field #infoBox FlatButt QLabel {
    margin: 0px;
}

Field #infoBox QProgressBar {
    border: 0px solid #5C6BC0;
    border-radius: 0px;
    background: #5C6BC0;
    text-align: center;
    min-height: 3px;
    max-height: 3px;
}

Field #infoBox QProgressBar::chunk{
    background: #FFE082;
    border-radius: 0px;
    border: none;
}

Field #infoBox #phaseLeft,
Field #infoBox #phaseRight {
    background: #FFF8E1;
}

Field #infoBox #phaseLeft::chunk,
Field #infoBox #phaseRight::chunk {
    background: #FF8F00;
}





#rightBarBox {
    background: #607D8B;
}

#rightBarBox QScrollBar::handle:vertical {
    background-color: #37474F;
}

#rightBarBox #sideSelector {
    min-height: 40px;
    padding: 0;
    margin: 0;
    border-radius: 0px;
}

#rightBarBox #sideSelector FlatButt {
    background: #CFD8DC;
    border: 0px solid #3F51B5;
    border-bottom: 2px solid #3F51B5;
