/******************************************************************************

    Intro Menu

******************************************************************************/

Intro,
Choice,
SoloChoice,
JoinChoice,
HostChoice,
NetChoice {
    margin: 0px 0px 0px 0px;
    padding: 0 0 30px 0;
}

Intro #introBox,
Choice #introBox,
SoloChoice #introBox,
JoinChoice #introBox,
HostChoice #introBox,
NetChoice #introBox {
    margin: 0px 30px;
    border-radius: 2px;
    background: #E0F2F1;
    max-width: 600px;
}

Intro FlatExpButt,
Choice FlatExpButt,
SoloChoice FlatExpButt,
JoinChoice FlatExpButt,
HostChoice FlatExpButt,
NetChoice FlatExpButt {
    margin: 0px 30px;
   	padding: 0px 10px;
} 

Intro FlatExpButt > *,
Choice FlatExpButt > *,
SoloChoice FlatExpButt > *,
JoinChoice FlatExpButt > *,
HostChoice FlatExpButt > *,
NetChoice FlatExpButt > * {
    margin: 0px 30px;
}


Intro FlatExpButt QToolTip,
Choice FlatExpButt QToolTip,
SoloChoice FlatExpButt QToolTip,
JoinChoice FlatExpButt QToolTip,
HostChoice FlatExpButt QToolTip,
NetChoice FlatExpButt QToolTip {
    margin: 0px;
    padding: 5px 10px;
}

Intro #introBox > QFrame,
Choice #introBox > QFrame, 
SoloChoice #introBox > QFrame {
