QToolTip {
    margin: 0px;
    padding: 0px 10px;
    background: #E0F2F1;
    border: none;
}



/******************************************************************************
    Text Inputs
******************************************************************************/

QTextEdit,
QLineEdit,
QPlainTextEdit {
    padding: 5px;
    color: #263238;
    border-radius: 0px;
    border: none;
}



/******************************************************************************
    Combo
******************************************************************************/

Combo {
    selection-background-color: #FFD740;
    border: none;
    border-radius: 0px;
    padding: 5px 10px 5px 10px;
    background-color: #FFFFFF;
    color: black;
    min-width: 75px;
}

Combo:hover,
Combo:focus {
    background: #FFE082;
}

Combo:on {
    selection-background-color: #FF6F00;
    padding: 0px;
}

Combo QAbstractItemView {
    background-color: #FFF8E1;
    border: none;
    selection-background-color: #18465d;
}

Combo::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border:none;
}


Combo::up-arrow,
Combo::down-arrow {
    image: url(img/rc/down_arrow_disabled.png);
}

Combo::down-arrow:on,
Combo::down-arrow:hover,
Combo::down-arrow:focus {
    image: url(img/rc/down_arrow.png);
}



/******************************************************************************
    SpinBox
******************************************************************************/

QAbstractSpinBox {
    padding: 5px;
    border: none;
    background-color: #FFF;
    color: #000;
    border-radius: 0px;
    min-width: 75px;
}

QAbstractSpinBox:hover,
QAbstractSpinBox:focus {
    background-color: #FFE082;
}

QAbstractSpinBox:up-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: top right;
}

QAbstractSpinBox:down-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: bottom right;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url(img/rc/up_arrow_disabled.png);
    width: 10px;
    height: 10px;
}

QAbstractSpinBox::up-arrow:hover {
    image: url(img/rc/up_arrow.png);
}

QAbstractSpinBox::down-arrow,
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off {
    image: url(img/rc/down_arrow_disabled.png);
    width: 10px;
    height: 10px;
}

QAbstractSpinBox::down-arrow:hover {
    image: url(img/rc/down_arrow.png);
}



/******************************************************************************
    ScrollBars
******************************************************************************/

QAbstractScrollArea {
    border:none;
}

QScrollBar:vertical {
    width: 18px;
    margin: 15px 5px 15px 5px;
    border: none;
    background: #E8EAF6;
    border-radius: 3px;
}

QScrollBar::handle:vertical {
    background-color: #455A64;
    min-height: 5px;
    border-radius: 3px;
}

QScrollBar::sub-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url(img/rc/up_arrow_disabled.png);
    height: 10px;
    width: 10px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
    margin: 3px 0px 3px 0px;
    border-image: url(img/rc/down_arrow_disabled.png);
    height: 10px;
    width: 10px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on {
    border-image: url(img/rc/up_arrow.png);
    height: 10px;
    width: 10px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
    border-image: url(img/rc/down_arrow.png);
    height: 10px;
    width: 10px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    background: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}



QScrollBar:horizontal {
    height: 18px;
    margin: 5px 15px 5px 15px;
    border: none;
    border-radius: 3px;
    background-color: #E8EAF6;
}

QScrollBar::handle:horizontal {
    background-color: #607D8B;
    min-width: 5px;
    border-radius: 3px;
}

QScrollBar::add-line:horizontal {
    margin: 0px 3px 0px 3px;
    border-image: url(img/rc/right_arrow_disabled.png);
    width: 10px;
    height: 10px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    margin: 0px 3px 0px 3px;
    border-image: url(img/rc/left_arrow_disabled.png);
    height: 10px;
    width: 10px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
    border-image: url(img/rc/right_arrow.png);
    height: 10px;
    width: 10px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
    border-image: url(img/rc/left_arrow.png);
    height: 10px;
    width: 10px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
    background: none;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}




/******************************************************************************
    CheckBox
******************************************************************************/

QCheckBox {
    outline: none;
}

QCheckBox::indicator {
    width: 15px;
    height: 15px;
    border: 2px solid #7986CB;
    border-radius: 3px;
    padding: -1px;
    margin: 1px;
    background: transparent;
}

QCheckBox::indicator:checked {
    padding: 0px;
    margin: 0px;
    background: #7986CB;
    border-image: url(img/rc/checkbox.png);
}

QCheckBox::indicator:hover, 
QCheckBox::indicator:focus,
QCheckBox::indicator:pressed {
    background: #FFD54F;
}





/*****************************************************************
    editeur_de_carte
******************************************************************/

cardEditor {
	border: none;
	outline: none;
	border-radius: 3px;
	background: #607D8B;
}



/*****************************************************************
    Shadow Button
******************************************************************/


DarkButt,
ShadowButt {
    border: none;
    outline: none;
    border-radius: 2px;
    background: #CFD8DC;
}

DarkButt QLabel,
ShadowButt QLabel {
    color: #000;
    margin: 0 10px;
}

DarkButt:hover,
DarkButt:focus,
ShadowButt:hover, 
ShadowButt:focus {
    background: #FFD54F;
}

DarkButt:pressed,
ShadowButt:pressed {
    background: #FFCA28;
}




/*****************************************************************
    Flat Button
******************************************************************/

FlatButt {
    border: none;
    outline: none;
    border-radius: 0px;
	background: transparent;
}

FlatButt QLabel {
    margin: 0;
}

FlatButt:hover,
FlatButt::focus {
    background: #80D8FF;
}
 
FlatButt:pressed {
    background: #0288D1;
}





/*****************************************************************
    Expanding Flat Button
******************************************************************/

FlatExpButt {
    border: none;
    outline: none;
    border-radius: 0px;
	background: #E0F2F1;
}

FlatExpButt QLabel {
    color: #000;
    margin: 0px 30px;
}


FlatExpButt:hover,
FlatExpButt:focus {
    background: #FFE082;
}
 
FlatExpButt:pressed {
    background: #FFD54F;
}




/*****************************************************************
    Main Window
******************************************************************/

Window {
    background: #607D8B;
}

Window > QWidget {
    border-image: url(img/wallpaper/wallpaper.png);
}




